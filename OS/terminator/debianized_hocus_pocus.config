#
# Terminator Config File
# Adjusted for Debianized Hocus Pocus theme.
#
# Author : Mademoiselle Parker
# Github : https://github.com/mlleparker/DevStuff
#
# Version : 0.0.0.0 SubZero
#
# How to use :
#  1. Rename this file in 'config'
#  2. Put it in ~/config/terminator/
#  3. Enjoy
#

[global_config]
  handle_size = 2
  enabled_plugins = TerminalShot,
  always_split_with_profile = True
  title_inactive_fg_color = "#717171"
  title_transmit_bg_color = "#d60451"
  inactive_color_offset = 0.4
  title_inactive_bg_color = "#12171d"
[keybindings]
[profiles]
  [[default]]
    palette = "#000000:#cd0000:#00cd00:#cdcd00:#1e90ff:#cd00cd:#00cdcd:#e5e5e5:#4c4c4c:#ff0000:#00ff00:#ffff00:#4682b4:#ff00ff:#00ffff:#ffffff"
    background_darkness = 0.48
    audible_bell = True
    background_type = transparent
    background_image = None
    cursor_color = "#d60451"
    foreground_color = "#bbbbbb"
    scroll_on_output = False
    cursor_shape = ibeam
    copy_on_selection = True
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
